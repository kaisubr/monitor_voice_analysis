                  <Button fx:id="startLearn_button" disable="true" mnemonicParsing="false" text="Start" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Button fx:id="stopLearn_button" disable="true" mnemonicParsing="false" text="Stop recording" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <Label text="Start recording" GridPane.columnIndex="2" GridPane.halignment="RIGHT" />
                  <Button fx:id="startTest_button" mnemonicParsing="false" text="Start" GridPane.columnIndex="3" />
                  <Button fx:id="stopTest_button" disable="true" mnemonicParsing="false" text="Stop recording" GridPane.columnIndex="3" GridPane.rowIndex="1" />
                  <Label text="Prediction" GridPane.columnIndex="2" GridPane.halignment="RIGHT" GridPane.rowIndex="2" />
                  <Label text="Confidence" GridPane.columnIndex="2" GridPane.halignment="RIGHT" GridPane.rowIndex="3" />
                  <Label fx:id="confidence_label" text="Unknown" GridPane.columnIndex="3" GridPane.rowIndex="3" />
                  <Label fx:id="prediction_label" prefHeight="30.0" prefWidth="121.0" text="Unknown" wrapText="true" GridPane.columnIndex="3" GridPane.rowIndex="2" />
