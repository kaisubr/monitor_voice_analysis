<BorderPane maxHeight="350.0" maxWidth="600.0" minHeight="500.0" minWidth="600.0" prefHeight="350.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
   <top>
      <AnchorPane fx:id="header_pane" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="header_label" layoutX="258.0" layoutY="19.0" text="Monitor" textAlignment="CENTER">
               <font>
                  <Font name="Segoe UI Light" size="25.0" />
               </font>
            </Label>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </AnchorPane>
   </top>
   <center>
      <AnchorPane fx:id="content_pane" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Label layoutX="14.0" layoutY="4.0" prefHeight="38.0" prefWidth="485.0" text="To learn, enter phone number and press start (left column). To test, press start on the right." wrapText="true" />
            <GridPane hgap="10.0" layoutX="14.0" layoutY="72.0" prefHeight="157.0" prefWidth="530.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="216.0" minWidth="10.0" prefWidth="116.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="300.0" minWidth="10.0" prefWidth="133.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="300.0" minWidth="5.0" prefWidth="117.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="300.0" minWidth="10.0" prefWidth="122.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints maxHeight="58.0" minHeight="10.0" prefHeight="37.0" vgrow="SOMETIMES" />
                  <RowConstraints maxHeight="58.0" minHeight="10.0" prefHeight="37.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="121.0" minHeight="10.0" prefHeight="35.0" vgrow="SOMETIMES" />
                  <RowConstraints maxHeight="121.0" minHeight="10.0" prefHeight="35.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="Start recording" GridPane.halignment="RIGHT" GridPane.rowIndex="1" />
